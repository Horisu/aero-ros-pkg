<!-- -*- mode: xml -*- -->
<launch>
  <include file="$(find jsk_pcl_ros)/launch/hsi_color_filter.launch">
    <arg name="INPUT" value="/stereo/points2"/>
    <arg name="DEFAULT_NAMESPACE" value="/stereo/hsi_color_filter/"/>
    <arg name="h_max" value="17"/>
    <arg name="h_min" value="0"/>
    <arg name="s_max" value="255"/>
    <arg name="s_min" value="235"/>
    <arg name="i_max" value="255"/>
    <arg name="i_min" value="19"/>
    <rosparam>
      /stereo/hsi_color_filter/euclidean_clustering/min_size: 50
      /stereo/hsi_color_filter/euclidean_clustering/max_size: 200
    </rosparam>
  </include>

  <node name="test_publish_single_tomato_tf" pkg="aero_tomato_vision"
  	type="test_publish_single_tomato_tf.py" output="screen">
  </node>
</launch>
