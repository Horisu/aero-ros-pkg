<?xml version="1.0"?>
<launch>

  <!--- Run bringup -->
  <include file="$(find aero_startup)/aero_move_base/launch/wheel_bringup.launch" />

  <!-- Run map navigation -->
  <arg name="map_file" default="$(find aero_startup)/aero_move_base/maps/map.yaml"/>
  <include file="$(find aero_startup)/aero_move_base/launch/wheel_bringup.launch">
    <arg name="map_file" value="$(arg map_file)" />
  </include>

</launch>
