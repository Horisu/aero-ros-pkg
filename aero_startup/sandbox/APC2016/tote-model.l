(defun make-tote ()
  (let (outer inner tote)
    (setq outer (make-cube (* 25.4 24) (* 25.4 15) (* 25.4 8))
	  inner (make-cube (* 25.4 21) (* 25.4 13.9) (* 25.4 7.8)))
    (send outer :translate (float-vector 0 0 (* 25.4 4.0)))
    (send inner :translate (float-vector 0 0 (* 25.4 4.1)))
    (setq tote (body- outer inner))
    (send tote :set-color :red)
    (instance bodyset-link :init (make-cascoords) :name "tote"
	      :bodies (list tote))
    )
  )
